{
  "plugins": {
    "autoprefixer": true,
    "postcss-mixins": true,
    "postcss-font-magician": true,
    "postcss-nesting": true,
    "postcss-clean": true,
    "@fullhuman/postcss-purgecss": {
      "content": [
        "./src/**/*.html",
        "./src/**/*.vue"
      ]
    },
    "postcss-preset-env": {
      "stage": 3,
      "features": {
        "nesting-rules": true
      }
    },
    "postcss-css-variables": true,
  }
}
